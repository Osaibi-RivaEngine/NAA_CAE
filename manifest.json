{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/vDevPreview/MicrosoftTeams.schema.json",
  "id": "00000000-0000-0000-0000-000000000000",
  "version": "1.0.0",
  "manifestVersion": "devPreview",
  "name": {
    "short": "NAA CAE Addin",
    "full": "NAA CAE Outlook Add-in"
  },
  "description": {
    "short": "Outlook add-in with Nested App Auth & Claims Challenge",
    "full": "An Outlook Office.js web add-in built with Vue.js and MSAL.js. Supports Nested App Authentication (NAA) and Continuous Access Evaluation (CAE) claims challenges."
  },
  "developer": {
    "name": "Contoso",
    "websiteUrl": "https://localhost:3000",
    "privacyUrl": "https://localhost:3000/privacy",
    "termsOfUseUrl": "https://localhost:3000/terms"
  },
  "icons": {
    "color": "assets/icon-color.png",
    "outline": "assets/icon-outline.png"
  },
  "accentColor": "#0078D4",
  "authorization": {
    "permissions": {
      "resourceSpecific": [
        {
          "name": "Mailbox.ReadWrite.User",
          "type": "Delegated"
        }
      ]
    }
  },
  "webApplicationInfo": {
    "id": "CLIENT ID",
    "resource": "api://localhost:3000/CLIENT ID"
  },
  "extensions": [
    {
      "requirements": {
        "scopes": ["mail"],
        "capabilities": [
          { "name": "Mailbox", "minVersion": "1.5" }
        ]
      },
      "runtimes": [
        {
          "requirements": {
            "capabilities": [
              { "name": "Mailbox", "minVersion": "1.5" }
            ]
          },
          "id": "TaskPaneRuntime",
          "type": "general",
          "code": {
            "page": "https://localhost:3000/taskpane.html"
          },
          "lifetime": "short",
          "actions": [
            {
              "id": "TaskPaneAction",
              "type": "openPage",
              "view": "taskpane"
            }
          ]
        },
        {
          "requirements": {
            "capabilities": [
              { "name": "Mailbox", "minVersion": "1.5" }
            ]
          },
          "id": "CommandsRuntime",
          "type": "general",
          "code": {
            "page": "https://localhost:3000/commands.html",
            "script": "https://localhost:3000/src/commands/commands.ts"
          },
          "lifetime": "short",
          "actions": [
            {
              "id": "ShowTaskpane",
              "type": "executeFunction"
            }
          ]
        }
      ],
      "ribbons": [
        {
          "contexts": ["mailRead", "mailCompose"],
          "tabs": [
            {
              "builtInTabId": "TabDefault",
              "groups": [
                {
                  "id": "NaaGroup",
                  "label": "NAA CAE",
                  "icons": [
                    { "size": 16, "url": "https://localhost:3000/assets/icon-16.png" },
                    { "size": 32, "url": "https://localhost:3000/assets/icon-32.png" },
                    { "size": 80, "url": "https://localhost:3000/assets/icon-80.png" }
                  ],
                  "controls": [
                    {
                      "id": "OpenTaskpane",
                      "type": "button",
                      "label": "NAA CAE",
                      "icons": [
                        { "size": 16, "url": "https://localhost:3000/assets/icon-16.png" },
                        { "size": 32, "url": "https://localhost:3000/assets/icon-32.png" },
                        { "size": 80, "url": "https://localhost:3000/assets/icon-80.png" }
                      ],
                      "supertip": {
                        "title": "NAA CAE Add-in",
                        "description": "Open the NAA CAE task pane"
                      },
                      "actionId": "TaskPaneAction"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
